{
    "require": {
        "asgrim/mini-mezzio": "^2.2",
        "laminas/laminas-config-aggregator": "^1.18",
        "laminas/laminas-inputfilter": "dev-service-manager-v4",
        "laminas/laminas-servicemanager": "4.5.x-dev",
        "league/commonmark": "^2.8@dev",
        "league/html-to-markdown": "^5.2@dev",
        "mezzio/mezzio-fastroute": "^3.13",
        "mezzio/mezzio-twigrenderer": "^2.17",
        "php-db/phpdb": "dev-x.x.2",
        "php-db/phpdb-adapter-sqlite": "0.1.x-dev",
        "twig/intl-extra": "^3.21",
        "twig/markdown-extra": "4.x-dev",
        "twilio/sdk": "^8.7",
        "vlucas/phpdotenv": "^5.6"
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "php -d upload_max_filesize=5242880 -d post_max_size=7340032 -S 0.0.0.0:8080 -t public/"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:ramchale/laminas-inputfilter.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:php-db/phpdb.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:php-db/phpdb-adapter-sqlite.git"
        }
    ]
}
